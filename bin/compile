#!/usr/bin/env bash

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3

if [ -f $ENV_DIR/PROC_NAME ]; then
  PROC_NAME=`cat $ENV_DIR/PROC_NAME`
  mv $BUILD_DIR/deploy/Procfile.$PROC_NAME $BUILD_DIR/Procfile
  echo "-----> Using Procfile.$PROC_NAME as Procfile"
else
  echo "-----> PROC_NAME not set in environment, doing nothing."
fi
